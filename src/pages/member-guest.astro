---
// Import header and footer to each page making it easier for the user to navigate between each page.
import Mainlayout from "../layouts/mainlayout.astro";
// Imports Carousel from PhotoSlider. Allows users to look at pictures when entering each page.
import PhotoSlider from "../components/photo-slider.astro";

// Adds view transistion to and between each page.
import { ViewTransitions } from "astro:transitions";

// Sets the company name.
let companyname = 'Whispering Woods';
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{companyname} - Member-Guest</title>

    <!-- Icon image to each page -->
    <link rel="icon" type="image/x-icon" href="/images/CroppedLogo2Black.png">

    <ViewTransitions />
</head>
<body>

    <Mainlayout>

        <PhotoSlider></PhotoSlider>

        <div class="container">
            <br>
            <h1>Member-Guest Tournament:</h1>

            <hr>

            <!-- TODO: Talk to Wyatt, Tony and Davis about what information wants to be put here. -->
            <p>More information on Member-Guest tournament comming soon.</p>
        </div>

    </Mainlayout>
    
  
</body>
</html>